{
    "version": 2,
    "buildCommand": "npm install -g @nestjs/cli;npm run build,npx prisma generate;echo 'hello world'",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}