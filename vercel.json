{
    "version": 2,
    "builds": [
        {


            "config": {
                "includeFiles": "src/**"
            },

            "postbuild": "prisma generate;npm install, npm run build",

            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}